def userInput():
        noGreenTandP = input("Enter Number of green Team and Probability of connections (n,p): ") #number of agents in the green team and probability of connections (n,p)
        noofGreyTandSpiesforRed = input("Enter Number of agents in theGrey Team and proportion of agents who are spies from the red team: ")
        uncertainty = input("Enter the Uncertainty interval of the agents. e.g. [-0.1,0.1] or [-0.5,0.5]: ")
        print(noGreenTandP,noofGreyTandSpiesforRed,uncertainty,uncertainty)


userInput()